Uses extra array leads to O(n) space.

class Solution {
public:
    void rotate(vector<int>& nums, int k) {
     int n = nums.size();
        vector<int> rotated(n, 0);

        k = k%n;

        for(int i=0; i<n; i++) {
            rotated[(i+k)%n] = nums[i];
        }

        for(int i=0; i<n; i++) {
            nums[i] = rotated[i];
        }   
    }
};
