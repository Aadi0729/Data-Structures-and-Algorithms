#include <iostream>
#include <vector>
#include <map>
using namespace std;

int removeDuplicates(vector<int>& nums) {
  int n = nums.size();
  
  map<int, int> mpp;
  
  for(int i=0; i<n; i++) {
    mpp[nums[i]]++;
  }
  
  int i=0;
  for(const auto& pair : mpp) {
    if(pair.second >= 2) {
      nums[i++] = pair.first;
      nums[i++] = pair.first;
    }
    else {
      nums[i++] = pair.first;
    }
  }
  
  return i;
}

int main() {
    int n;
    cin >> n;
    
    vector<int> nums(n);
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }
    
    int res = removeDuplicates(nums);
    
    cout << "k = " << res;
    
    return 0;
}
