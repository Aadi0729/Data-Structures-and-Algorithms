#include <iostream>
#include <vector>
using namespace std;

void moveZeroesToEnd(vector<int>& nums) {
    int n = nums.size();
    
    vector<int> temp(n);
    
    int j=0;
    int k=n-1;
    for(int i=0; i<n; i++) {
        if(nums[i] != 0) {
            temp[j] = nums[i];
            j++;
        }
        else {
            temp[k] = nums[i];
            k--;
        }
    }
    
    for(int i=0; i<n; i++) {
        nums[i] = temp[i];
    }
}

int main() {
    int n;
    cout << "Enter size: ";
    cin >> n;
    
    vector<int> nums(n);
    cout << "Enter elements: " << endl;
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }
    
    moveZeroesToEnd(nums);
    
    for(auto val : nums) {
        cout << val << " ";
    }
    
    return 0;
}
