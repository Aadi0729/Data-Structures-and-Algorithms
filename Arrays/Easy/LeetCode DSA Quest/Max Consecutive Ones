class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int n = nums.size();
        int cnt = 0;
        int maxCnt = 0;

        for(int i=0; i<n; i++) {
            if(nums[i] == 1) {
                cnt++;
                maxCnt = max(cnt, maxCnt);
            }
            else
                cnt = 0;
        }

        return maxCnt;
    }
};
