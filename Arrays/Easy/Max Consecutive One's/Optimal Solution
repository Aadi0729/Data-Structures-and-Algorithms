class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int n = nums.size();

        int cnt1 = 0;
        int maxCnt1 = 0;

        for(int i=0; i<n; i++) {
            if(nums[i] == 1) {
                cnt1++;
                maxCnt1 = max(cnt1, maxCnt1);
            }
            else
                cnt1 = 0;
        }

        return maxCnt1;
    }
};
