#include <iostream>
#include <vector>
#include <limits.h>
using namespace std;

int largestElement(vector<int>& nums) {
    int maxNum = INT_MIN;

    for(auto val : nums) {
        if(val > maxNum)
            maxNum = val;
    }
    return maxNum;
}

int main() {
    int n;
    cout << "Enter size: ";
    cin >> n;
    
    if(n <= 0) {
        cout << "Invalid size!";
        return 0;
    }
    
    vector<int> nums(n);
    
    cout << "Enter elements: ";
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }

    int largestNum = largestElement(nums);
    
    cout << "Largest number; " << largestNum;
    
    return 0;
}
