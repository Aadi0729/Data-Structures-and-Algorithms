#include <iostream>
#include <vector>
using namespace std;

int findSingleNumber(vector<int>& nums) {
  int n = nums.size();
  
  for(int i=0; i<n; i++) {
    bool flag = true;
    for(int j=0; j<n; j++) {
      if(i != j && nums[i] == nums[j]) {
        flag = false;
      }
    }
    if(flag == true) 
      return nums[i];
  }
  
  return -1;
}

int main() {
    int n;
    cin >> n;
    
    vector<int> nums(n);
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }
    
    int res = findSingleNumber(nums);
    
    cout << res << " is the number appeared once in the array.";
    
    return 0;
}
