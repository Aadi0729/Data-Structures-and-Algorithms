#include <iostream>
#include <vector>
#include <limits.h>
using namespace std;

int secondlargestElement(vector<int>& nums) {
    int firstMax = INT_MIN;
    int secondMax = INT_MIN;

    for(auto val : nums) {
        if(val > firstMax) {
            secondMax = firstMax;
            firstMax = val;
        }
        else if(val > secondMax && val < firstMax) {
            secondMax = val;
        }
    }
    
    if(secondMax == INT_MIN) {
        return INT_MIN;
    }
    
    return secondMax;
}

int main() {
    int n;
    cout << "Enter size: ";
    cin >> n;
    
    if(n <= 0) {
        cout << "Invalid size!";
        return 0;
    }
    
    if(n == 1) {
        cout << "Second largest element doesn't exist.";
        return 0;
    }
    
    vector<int> nums(n);
    
    cout << "Enter elements: ";
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }

    int secondlargestNum = secondlargestElement(nums);
    
    if(secondlargestNum == INT_MIN) {
        cout << "Second largest element doesn't exist.";
    }
    else {
        cout << "Second largest element is " << secondlargestNum;
    }
    
    return 0;
}
