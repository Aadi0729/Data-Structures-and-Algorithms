#include <iostream>
#include <vector>
using namespace std;

int findMissingNumber(vector<int>& nums) {
  int n = nums.size();
  
  int i=0;
  while(i <= n) {
    bool flag = true;
    for(int j=0; j<n; j++) {
      if(i == nums[j]) {
        flag = false;
        break;
      }
    }
    if(flag == true)
      return i;
    
    i++;
  }
  return -1;
}

int main() {
    int n;
    cin >> n;
    
    vector<int> nums(n);
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }
    
    int res = findMissingNumber(nums);
    
    cout << res << " is missing in nums.";
    
    return 0;
}
