// Covering all the cases including duplicates, single element array, empty array, INT_MIN values etc.

#include <bits/stdc++.h>
using namespace std;

int findSecondMax(vector<int>& nums) {
    int n = nums.size();

    if(n < 2) return -1;

    int max1 = nums[0];
    int max2;

    bool hasMax2 = false;

    for(int i=1; i<n; i++) {
        if(nums[i] > max1) {
            max2 = max1;
            max1 = nums[i];
            hasMax2 = true;
        }
        else if(nums[i] < max1) {
            if(!hasMax2 || nums[i] > max2) {
                max2 = nums[i];
                hasMax2 = true;
            }
        }
    }
    return hasMax2 ? max2 : -1;
}

int main() {
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif

    int n;
    cin >> n;

    vector<int> nums(n);
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }

    int max2 = findSecondMax(nums);

    cout << max2;
    
    return 0;
}