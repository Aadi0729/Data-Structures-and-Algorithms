#include <iostream>
#include <vector>
#include <limits.h>
using namespace std;

int findKthLargest(vector<int>& nums, int k) {
    int n = nums.size();
    
    int prevMax = INT_MAX;
    
    for(int i=0; i<k; i++) {
        int j;
        int currMax = INT_MIN;
        for(j=0; j<n; j++) {
            if(nums[j] < prevMax && nums[j] > currMax)
                currMax = nums[j];
        }
        
        if(currMax < prevMax)
            prevMax = currMax;
    }
    
    return prevMax;
}

int main() {
    int n;
    cout << "Enter the size ";
    cin >> n;
    
    if(n<=0) {
        cout << "Invalid size!";
        return 0;
    }
    
    vector<int> nums(n);
    cout << "Enter the values in nums: " << endl;
    
    for(int i=0; i<n; i++) {
        cin >> nums[i];
    }
    int k;
    cout << "Enter the k: ";
    cin >> k;
    
    int kthLargest = findKthLargest(nums, k);
    
    if(kthLargest == INT_MIN)
        cout << "Not found!";
    else
        cout << "The " << k << "th largest element is " << kthLargest;
    
    return 0;
}
